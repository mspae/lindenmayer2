{
  "name": "lindenmayer2",
  "type": "module",
  "description": "Yet another L-system implementation.",
  "version": "0.2.0",
  "source": "src/index.ts",
  "exports": {
    "require": "./dist/lindenmayer2.cjs",
    "default": "./dist/lindenmayer2.modern.js"
  },
  "main": "./dist/lindenmayer2.cjs",
  "module": "./dist/lindenmayer2.module.js",
  "unpkg": "./dist/lindenmayer2.umd.js",
  "author": "Martin Spencer <mail@martinspencer.de>",
  "license": "MIT",
  "scripts": {
    "build": "microbundle --worker",
    "dev": "microbundle --worker watch",
    "test": "jest",
    "test:debug": "node --inspect-brk=0.0.0.0 ./node_modules/jest/bin/jest.js",
    "test:watch": "jest --watch",
    "test:watch:debug": "node --inspect-brk=0.0.0.0 ./node_modules/jest/bin/jest.js --watchAll --runInBand",
    "prepare": "npm run test && npm run build"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@types/jest": "^27.4.1",
    "jest": "^27.5.1",
    "microbundle": "^0.14.2",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  },
  "dependencies": {}
}
